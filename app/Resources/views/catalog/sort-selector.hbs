<!-- start catalog/sort-selector.hbs -->
<select name="{{content.sortSelector.key}}" class="select-product-detail" required onchange="window.location = this.options[this.selectedIndex].value;">
{{#each sortSelector.list}}
  <option value="{{uri}}" {{#if selected}}selected{{/if}}>{{label}}</option>
{{/each}}
</select>
<!-- end catalog/sort-selector.hbs -->
